{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-native",
    "noEmit": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      // "@src/*": ["src/*"],
      "@themes/*": ["src/themes/*"],
      // "@app/*": ["src/app/*"],
      "@components": ["src/components"],
      "@constants/*": ["src/constants/*"],
      "@screens/*": ["src/screens/*"],
      "@images": ["./assets/images"],
      "@certificate": ["./assets/certificate"],
      "@svgs": ["./assets/svgs/index.js"],
      "@fonts/*": ["./assets/fonts/*"],
      "@animation/*": ["./assets/animation/*"],
      "@utils/*": ["src/utils/*"],
      "@router/*": ["src/router/*"],
      "@state*": ["src/utils/state"],
      "@authApp/*": ["./src/app/auth/*"],
      "@homeApp/*": ["./src/app/home/*"],
      "@profileApp/*": ["./src/app/profile/*"],
      "@archiveApp/*": ["./src/app/archive/*"],
      "@othersApp/*": ["./src/app/others/*"],
      "@surveyApp/*": ["./src/app/survey/*"],
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"],
  "extends": "@tsconfig/react-native/tsconfig.json"
}
